        <script>
            $(document).ready( function () {
                $('#jobsTable').DataTable({
                    responsive: {
                        details: {
                            display: $.fn.dataTable.Responsive.display.childRowImmediate,
                            type: 'none',
                            target: ''
                        }
                    },
                    fixedHeader: {
                        header: true,
                        // headerOffset: $('.navbar').height()
                    },
                    "pageLength": 25,
                    <% if(currentUser.username == 'nadia' || currentUser.username == 'kayla') { %>
                        "order": [[ 0, "desc" ]]
                    <% } else { %>
                        "order": [[ 4, "desc" ]]
                    <% } %>
                });
            } );
            $(document).ready( function () {
                $('#usersTable').DataTable({
                    responsive: {
                        details: {
                            display: $.fn.dataTable.Responsive.display.childRowImmediate,
                            type: 'none',
                            target: ''
                        }
                    },
                    fixedHeader: {
                        header: true,
